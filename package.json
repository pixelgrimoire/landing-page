{
  "name": "landing-page",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "prisma generate || echo prisma generate failed - continuing build",
    "dev": "next dev --turbopack",
    "build": "next build",
    "vercel-build": "(npm run migrate:deploy || npm run db:push) && npm run build",
    "start": "next start",
    "lint": "eslint",
    "prisma:generate": "prisma generate",
    "db:push": "prisma db push",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:status": "prisma migrate status",
    "wipe:db": "prisma db push --force-reset",
    "wipe:stripe": "node scripts/wipe-stripe.mjs",
    "wipe:clerk": "node scripts/wipe-clerk.mjs",
    "wipe:clerk:all": "node scripts/wipe-clerk.mjs --include-admin"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.32.2",
    "@clerk/themes": "^2.4.22",
    "@gsap/react": "^2.1.2",
    "@prisma/client": "^5.20.0",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^4.6.0",
    "@vercel/blob": "^2.0.0",
    "gsap": "^3.13.0",
    "next": "15.5.2",
    "nodemailer": "^6.9.14",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "prisma": "^5.20.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
